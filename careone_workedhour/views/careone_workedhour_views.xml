<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mrp_workorder_form_worked" model="ir.ui.view">
        <field name="name">mrp.workorder.form.worked</field>
        <field name="model">mrp.workorder</field>
        <field name="inherit_id" ref="careone_mrp.workorder_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='time_ids']/tree/field[@name='loss_id']" position="after">
                <field name="cost_per_hour"/>
                <field name="currency_id" invisible="1"/>
                <field name="total_cost"/>
            </xpath>

        </field>
    </record>

    <record id="view_mrp_kanban_inherit_state_grp" model="ir.ui.view">
        <field name="name">MRP Kanban Group State Group</field>
        <field name="model">mrp.production</field>
        <field name="type">kanban</field>
        <field name="inherit_id" ref="mrp.mrp_production_kanban_view"/>
            <field name="arch" type="xml">
                <xpath expr="//kanban" position="attributes">
                    <attribute name="default_group_by">state_temp</attribute>
                </xpath>
            </field>
        </record>


